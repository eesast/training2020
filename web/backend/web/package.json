{
  "name": "sast-web",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --transformIgnorePatterns \"node_modules/(?!(antd|rc-pagination|rc-calendar|swagger-ui-react)/)\"",
    "eject": "react-app-rewired eject",
    "postinstall": "patch-package",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint --ext .js,.ts src/",
    "typecheck": "tsc --noUnusedLocals"
  },
  "dependencies": {
    "antd": "3.26.15",
    "axios": "0.19.2",
    "clipboard": "2.0.6",
    "jwt-decode": "2.2.0",
    "marked": "0.8.2",
    "md2wx": "3.1.2",
    "moment": "2.29.2",
    "rc-queue-anim": "1.8.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-infinite-scroller": "1.2.4",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "swagger-ui-react": "3.25.0",
    "typesafe-actions": "5.1.0",
    "xlsx": "0.15.6"
  },
  "devDependencies": {
    "@types/clipboard": "2.0.1",
    "@types/dompurify": "2.0.1",
    "@types/jest": "25.2.1",
    "@types/jwt-decode": "2.2.1",
    "@types/marked": "0.7.4",
    "@types/node": "13.13.0",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-infinite-scroller": "1.2.1",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.4",
    "babel-plugin-import": "1.13.0",
    "customize-cra": "0.9.1",
    "husky": "4.2.5",
    "less": "3.11.1",
    "less-loader": "5.0.0",
    "lint-staged": "10.1.4",
    "patch-package": "6.2.2",
    "prettier": "2.0.4",
    "react-app-rewired": "2.1.5",
    "react-scripts": "3.4.1",
    "redux-devtools-extension": "2.13.8",
    "source-map-explorer": "2.4.2",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint && yarn typecheck"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
